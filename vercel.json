{
  "functions": {
    "api/bot.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    { "src": "api/bot.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/webhook", "dest": "/api/bot.py" },
    { "src": "/set_webhook", "dest": "/api/bot.py" },
    { "src": "/(.*)", "dest": "/api/bot.py" }
  ]
}
